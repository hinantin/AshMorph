      ###################################

      define PWLenitionRule1 [ "@LEN@"p (->) "@LEN@"v || [{o}] _ [{i}|{a}] ]; 
                                                                            # nopito (in our lexicon) -> no@LEN@pito -> no@LEN@vito -> novito 
      define PWLenitionRule2 [ "@LEN@"p (->) "@LEN@"v || [{i}] _ [{i}|{a}] ]; 
                                                                            # ipanko (in our lexicon) -> i@LEN@panko -> i@LEN@vanko -> ivanko 
      define PWLenitionRule3 [ "@LEN@"p -> "@LEN@"v || [{a}] _ [{i}] ]; 
                                                                            # apinkatsarite (in our lexicon) -> a@LEN@pinkatsarite -> a@LEN@vinkatsarite -> avinkatsarite 
      define KJLenitionRule1 [ "@LEN@"k (->) "@LEN@"y || _ [{o}|{a}|{e}] ]; 
                                                                            # optional rule: noyokani, ikentsike  
      define KLenitionRule2 [ "@LEN@"k (->) "@LEN@" || [{o}] _ [{i}|{e}|{a}|{o}] ]; 
                                                                            # nokitzi (in our lexicon) -> no@LEN@kitzi -> no@LEN@itzi -> noitzi
                                                                            # nokanire (in our lexicon) -> no@LEN@kanire -> no@LEN@anire -> noanire
                                                                            # nokonta (in our lexicon) -> no@LEN@onta -> no@LEN@onta -> noonta 
                                                                            # okomaro (in our lexicon) -> o@LEN@omaro -> o@LEN@omaro -> oomaro-janiqui
                                                                            # oketsa (in our lexicon) -> o@LEN@etsa -> o@LEN@etsa -> o-etsa-re-ki
      define KLenitionRule3 [ "@LEN@"k (->) "@LEN@" || [{i}] _ [{i}] ]; 
                                                                            # ikishi (in our lexicon) -> i@LEN@kishi -> i@LEN@ishi -> iishi 
      define KLenitionRule4 [ "@LEN@"k (->) "@LEN@" || [{p}] _ [{i}] ]; 
                                                                            # pkitsare (in our lexicon) -> p@LEN@kitsare -> p@LEN@itsare -> pitsare 
      define KLenitionRule5 [ "@LEN@"k (->) "@LEN@" || [{o}] _ [{i}] ]; ! oitsoki -> o=kitsoki 
      define KLenitionRule6 [ "@LEN@"k (->) "@LEN@" || [{a}] _ [{i}] ]; ! aitsoki -> a=kitsoki 

      define Lenition [ PWLenitionRule1 .o. PWLenitionRule2 .o. PWLenitionRule3 
 .o. KJLenitionRule1 
 .o. KLenitionRule2 
 .o. KLenitionRule3 
 .o. KLenitionRule4 
 .o. KLenitionRule5 
 .o. KLenitionRule6 
];

