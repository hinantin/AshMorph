      define TEpenthesisRule9 [ "@EP" (->) {t}"@EP" || a _ i ]; # o-kametsa-t-i
      define TEpenthesisRule10 [ "@EP" (->) {t}"@EP" || e _ y ]; # no-ñatsa-bae-t-ya
