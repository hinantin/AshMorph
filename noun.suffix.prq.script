define N=S=SUPERL [
# otsiteninkataki / o- =tsiteni -nka -taki / 3n.m.poss- =to.get.dark -nmz -superl / EN: 'totally.dark'  
  "[--][-taki][+SUPERL]" : [{taki}]
| "[--][-tanaki][+SUPERL]" : [{tanaki}]
